{
  "name": "upsert-tagged-releases",
  "version": "1.0.0",
  "description": "Create tags based on version",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "npm install && npm run build",
    "build": "esbuild ./src --outfile=dist/index.js --bundle --platform=node --sourcemap",
    "test": "jest .",
    "test:watch": "jest . --watch",
    "format": "prettier --write **/*.js",
    "format-check": "prettier --check **/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "all": "npm run lint:fix && npm run format && npm run build"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Brady Clifford",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{js,yml,json,ts,md}": [
      "prettier --write"
    ],
    "*.{yml,yaml}": [
      "yamllint"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10",
    "@actions/github": "^5.1.1",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20",
    "@babel/preset-env": "^7.20",
    "@babel/preset-typescript": "^7.18",
    "@octokit/types": "^9.0.0",
    "@types/jest": "^29.4.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.49",
    "@typescript-eslint/parser": "^5.49",
    "babel-jest": "^29.4",
    "esbuild": "^0.17",
    "eslint": "^8.33",
    "husky": "^8.0.3",
    "jest": "^29.4",
    "lint-staged": "^13.1",
    "prettier": "^2.8",
    "yaml-lint": "^1.7"
  }
}
